{
    "env": {
        "es6": true,
        "node": true
    },
		"extends": [ "eslint:recommended", "react/recommended"],
		"plugins": ["jest", "react"],
    "rules": {
        "indent": [
            "error",
            "tab"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}
