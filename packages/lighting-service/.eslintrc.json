{
    "env": {
				"es6": true,
				"jest": true,
        "node": true
    },
		"extends": "eslint:recommended",
		"plugins": ["jest"],
    "rules": {
        "indent": [
            "error",
            "tab"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}
